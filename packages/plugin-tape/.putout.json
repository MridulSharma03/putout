{
    "printer": "putout",
    "rules": {
        "putout/create-test": ["on", {
            "add": [
                ["printer", "putout"]
            ]
        }]
    },
    "match": {
        "README.md": {
            "declare": "off",
            "regexp/apply-literal-notation": "off"
        }
    }
}
