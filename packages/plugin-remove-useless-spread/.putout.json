{
    "printer": "putout",
    "rules": {
        "putout/create-test": ["on", {
            "add": [
                ["printer", "putout"]
            ]
        }]
    },
    "match": {
        "*.md": {
            "remove-useless-spread": "off",
            "for-of/remove-useless": "off",
            "convert-array-copy-to-slice": "off"
        }
    }
}
